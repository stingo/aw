            <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption ">
                <img class="rounded" src="<%= @clothing.clothingimages[0].url(:large) %>" alt="<%= @clothing.name %>" >
              </div>
            </div>
            <!-- End Item -->


             <% end %>

            <% if @clothing.clothingimages[1] %>

            <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption">
                 <img class="rounded" src="<%= @clothing.clothingimages[1].url(:large) %>" alt="<%= @clothing.name %>" >
              </div>
            </div>
            <!-- End Item -->

             <% end %>

            <% if @clothing.clothingimages[2] %>

            <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption">
                
                

                <img class="rounded" src="<%= @clothing.clothingimages[2].url(:large) %>" alt="<%= @clothing.name %>">

              </div>
            </div>
            <!-- End Item -->

             <% end %>

            <% if @clothing.clothingimages[3] %>

            <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption">
                
                

                <img class="rounded" src="<%= @clothing.clothingimages[3].url(:large) %>" alt="<%= @clothing.name %>">

              </div>
            </div>
            <!-- End Item -->

             <% end %>

            <% if @clothing.clothingimages[4] %>


                        <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption">
                
                

                <img class="rounded" src="<%= @clothing.clothingimages[4].url(:large) %>" alt="<%= @clothing.name %>">

              </div>
            </div>
            <!-- End Item -->

             <% end %>


            <% if @clothing.clothingimages.count == 0 %>

            <!-- Item -->
            <div class="cbp-item">
              <div class="cbp-caption">
                
                

                <img class="rounded" src="https://s3.amazonaws.com/proinsidegh/static/noimage_upfrica_750_420.jpg" alt="<%= @clothing.name %>">

              </div>
            </div>
            <!-- End Item -->


            <% end %>







