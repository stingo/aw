                                          <% else %>
                                          <%= link_to "Bookmarked", clothing_bookmarks_path(@clothing), method: :post, 
                                          remote: true, :class=>"text-muted" %>
                                          <% end %>
                                        </div>
                                       
              


            </div>

            <hr class="my-5">

            <!-- overview -->


            <div class="mb-6">
            <h3 class="h6">Overview</h3>
            <p ><%= sanitize(@clothing.overview, tags: %w(strong em p br div ul ol li)).html_safe if @clothing.overview %> </p>
            </div>



