               data-sticky-view="lg"
               data-start-point="#stickyBlockStartPoint"
               data-end-point="#stickyBlockEndPoint"
               data-offset-top="80"
               data-offset-bottom="130">
            <div class="mb-6">
              <h1 class="h4 text-primary font-weight-semi-bold"><%= @clothing.name.titleize %>   <%=number_to_human_size(@clothing.clothingimages[0].size) %>, <%=number_to_human_size(@clothing.clothingimages[1].size)  %>, <%= @clothing.user.email %></h1>

