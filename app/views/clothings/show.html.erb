                </div>
                <div class="media-body">
                  <small class="text-muted">
                               <%= link_to W.friendly.find(@clothing.w_id).name, w_path(@clothing.w.slug), :class => ""  %>

                  </small>
                </div>
              </li>

<% end %>


<% if @clothing.fashion_id %>

              <li class="media mb-0">
                <div class="d-flex w-40 w-sm-30">
                  <label class="h6 small d-block text-uppercase text-dark">Fasion</label>
                </div>
                <div class="media-body">
                  <small class="text-muted">

                               <%= link_to Fashion.friendly.find(@clothing.fashion_id).name, fashion_path(@clothing.fashion.slug), :class => ""  %>


                  </small>
                </div>
              </li>

<% end %>








              <li class="media mb-0">
                <div class="d-flex w-40 w-sm-30">
                 <label class="h6 small d-block text-uppercase text-dark">Tagged</label>
                </div>
                <div class="media-body">
                  <small class="text-muted">
                     <% if @clothing.tag_ids.present? %>

                                      <div class="tag">


                                         <span class=""><%= raw(@clothing.tag_list.map { |t| link_to t, tag_path(t) }.join(', ')) %>

                                        

                                       </div>

                                         <% end %>
                  </small>
                  
                </div>
              </li>

             


            </ul>
            <!-- End List -->
                


                                        <% if signed_in? && (current_user== @clothing.user || current_user.admin?) %>

                                        <i class="fa fa-pencil-square-o" aria-hidden="true"></i>&nbsp<%= link_to "Edit", edit_clothing_path(@clothing), class: "" %>&nbsp &nbsp 
