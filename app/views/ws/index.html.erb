<p id="notice"><%= notice %></p>

<h1>Ws</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ws.each do |w| %>
      <tr>
        <td><%= w.name %></td>
        <td><%= link_to 'Show', w %></td>
        <td><%= link_to 'Edit', edit_w_path(w) %></td>
        <td><%= link_to 'Destroy', w, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New W', new_w_path %>
